{
  "name": "basic-webpack",
  "version": "1.0.0",
  "description": "An experimentation repo for creating a prototype whose foundation is a react-based technology stack.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./config/webpack.config.babel.js --progress --inline --hot --history-api-fallback --content-base ./tmp",
    "dev:test": "webpack-dev-server --inline --hot --config ./config/webpack.test.config.babel.js --port 8081 --open",
    "git:unpushed-files": "git log @{u}.. --name-only --pretty=\"format:\" | sort | uniq | grep -v \"^$\"",
    "git:unpushed-of-type": "npm run -s git:unpushed-files | grep",
    "lint:js": "npm run -s git:unpushed-of-type -- js$ | xargs eslint",
    "lint:css": "npm run -s git:unpushed-of-type -- css$ | xargs stylelint",
    "prod:webpack-compile-server": "DEBUG=webpack rm -rf dist tmp && webpack --progress --config ./config/webpack.server.config.babel.js ",
    "prod:webpack-compile-client": "NODE_ENV=production webpack --progress --config ./config/webpack.config.babel.js ",
    "prod:run-server": "DEBUG=server node tmp/server.bundle.js",
    "prod": "npm run prod:webpack-compile-server && npm run prod:webpack-compile-client && npm run prod:run-server ",
    "start": "npm run dev",
    "test": "mocha"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.6.2",
    "material-ui": "latest",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^4.0.0-alpha.4",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.1.0",
    "redux-form-material-ui": "^4.1.0",
    "redux-thunk": "^2.1.0",
    "restify": "^4.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "debug": "^2.2.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^15.1.1",
    "jsdom": "^9.5.0",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.0.2",
    "mocha-loader": "^1.0.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "postcss-reporter": "^1.4.1",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.3.1",
    "redux-logger": "^2.7.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "stylelint-webpack-plugin": "^0.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2",
    "webpack-notifier": "^1.4.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sinequanon/webpack-react.git"
  },
  "bugs": {
    "url": "https://github.com/sinequanon/webpack-react/issues"
  },
  "homepage": "https://github.com/sinequanon/webpack-react#readme"
}
